        "import sympy as sp\n",
        "\n",
        "i, n = sp.symbols(\"i n\", int=True, positive=True)\n",
        "x = sp.IndexedBase(\"x\", real=True)\n",
        "\n",
        "S = sp.Sum(x[i+1] - x[i],(i,0,n-1))\n",
        "S.doit()"
      ],
